{
    "name": "mmikkel/incognito-field",
    "description": "PlainText drop-in replacement that can be set to disabled, hidden or readonly.",
    "type": "craft-plugin",
    "version": "1.3.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "incognito field"
    ],
    "support": {
        "docs": "https://github.com/mmikkel/IncognitoField-Craft3/blob/master/README.md",
        "issues": "https://github.com/mmikkel/IncognitoField-Craft3/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mats Mikkel Rummelhoff",
            "homepage": "http://vaersaagod.no"
        }
    ],
    "require": {
        "craftcms/cms": "^3.7.0|^4.0.0-beta.3",
        "php": "^7.2.5|^8.0.2"
    },
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "mmikkel\\incognitofield\\": "src/"
        }
    },
    "extra": {
        "name": "Incognito Field",
        "handle": "incognito-field",
        "schemaVersion": "1.1.1",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/mmikkel/IncognitoField-Craft3/master/CHANGELOG.md",
        "class": "mmikkel\\incognitofield\\IncognitoField"
    }
}
